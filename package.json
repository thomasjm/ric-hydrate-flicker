{
  "name": "codedown-server-frontend-parcel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "apollo": "apollo client:codegen app/client/graphql --target typescript --outputFlat",
    "dev": "npm run generate-imported-components && node ./dev-server.js",
    "dev:server": "nodemon -e js,jsx,html --ignore dist --ignore app/imported.js --exec \"npm run build && npm run start\"",
    "lint": "prettier-standard 'app/**/*.js' 'app/**/*.jsx' 'server/**/*.js' --lint",
    "lint:fix": "prettier-standard 'app/**/*.js' 'app/**/*.jsx' 'server/**/*.js' --lint --fix",
    "format": "prettier-standard 'app/**/*.js' 'app/**/*.jsx' 'server/**/*.js' --format",
    "build": "rimraf dist && npm run generate-imported-components && npm run create-bundles && cp -r app/static ./dist",
    "create-bundles": "concurrently \"npm run create-bundle:client\" \"npm run create-bundle:server\"",
    "create-bundle:client": "cross-env BABEL_ENV=client parcel build app/index.html -d dist/client --no-source-maps --no-minify --public-url /dist/client",
    "create-bundle:server": "cross-env BABEL_ENV=server parcel build server/index.tsx -d dist/server --no-source-maps --no-minify --public-url /dist/client --target=node --bundle-node-modules",
    "create-bundle:client:no-minify": "cross-env BABEL_ENV=client parcel build app/index.html -d dist/client --no-source-maps --no-minify --public-url /dist/client",
    "create-bundle:server:no-minify": "cross-env BABEL_ENV=server parcel build server/index.tsx -d dist/server --no-source-maps --no-minify --public-url /dist/client --target=node --bundle-node-modules",
    "generate-imported-components": "imported-components app app/imported.ts",
    "node2nix": "node2nix --development --lock package-lock.json",
    "start": "node dist/server",
    "test": "cross-env BABEL_ENV=test jest --config jest.json",
    "test:watch": "cross-env BABEL_ENV=test jest --config jest.json --watch",
    "ts": "tsc -w -p . --noEmit --pretty",
    "upgrade-interactive": "npm-check --update"
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "author": "",
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/react-fontawesome": "0.0.18",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "bunyan": "^1.8.14",
    "clipboard-polyfill": "^3.0.1",
    "codemirror": "^5.51.0",
    "cross-fetch": "^3.0.5",
    "d3": "4.10.0",
    "exenv": "^1.2.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "graphql": "^15.3.0",
    "highlight.js": "^10.1.2",
    "http-proxy-middleware": "^1.0.5",
    "iframe-resizer": "^4.2.11",
    "immutability-helper": "^3.1.1",
    "javascript-time-ago": "^2.0.13",
    "llog": "^0.2.0",
    "lodash": "^4.17.20",
    "material-ui-popup-state": "^1.6.1",
    "node-fetch": "^2.6.0",
    "notistack": "^0.9.17",
    "pino": "^6.5.1",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-animate-height": "^2.0.23",
    "react-circular-progressbar": "^2.0.3",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-filepond": "^7.0.1",
    "react-google-login": "^5.1.21",
    "react-gravatar": "^2.6.3",
    "react-helmet-async": "^1.0.6",
    "react-imported-component": "^6.3.11",
    "react-json-view": "git+https://github.com/thomasjm/react-json-view.git#4fd777f032e5f6b29685f39fdbed9135b1a4a3c7",
    "react-markdown": "^4.3.1",
    "react-number-format": "^4.4.1",
    "react-popper": "^0.10.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.9.0",
    "react-swipeable-views": "^0.13.9",
    "react-syntax-highlighter": "^13.5.0",
    "react-typist": "^2.0.5",
    "react-visibility-sensor": "^5.1.1",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.1",
