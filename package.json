    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "apollo": "^2.30.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.3.0",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "4.2.5",
    "isomorphic-cookie": "^1.2.4",
    "jest": "^26.4.0",
    "lint-staged": "10.2.11",
    "node2nix": "git+https://github.com/thomasjm/node2nix.git#0fc727070dd8e5910601ab0ccc1dd0accc42cf05",
    "nodemon": "2.0.4",
    "npm-check": "^5.9.2",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-markdown": "^0.4.2",
    "prettier-standard": "16.4.1",
    "react-hot-loader": "4.12.21",
    "rimraf": "3.0.2",
    "typescript": "^3.9.7"
  }
}
